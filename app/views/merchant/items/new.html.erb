<h1>New Item Page</h1>

<%= form_with url: merchant_items_path, method: :post do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>
  <%= f.label :description, "Description" %>
  <%= f.text_field :description %>
  <%= f.label :unit_price, "Current Price" %>
  <%= f.number_field :unit_price %>
  <%= f.submit "Submit" %>
<% end %>